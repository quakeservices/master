{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:enableMajor",
    "group:all"
  ],
  "enabledManagers": [
    "dockerfile",
    "npm",
    "pip-compile"
  ],
  "pip-compile": {
    "fileMatch": [
      "(^|/)requirements.*\\.in$"
    ]
  }
}
