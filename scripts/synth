set -euo pipefail

cd $(dirname $0)/..

if ! which cdk; then
    echo "CDK command not found"
    exit 1
fi

if test -d .env; then
    source .env/bin/activate && \
    pip install -r requirements.txt
else
    python3 -m venv .env && \
    source .env/bin/activate && \
    pip install -r requirements.txt
fi

cdk synth
