{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:all"
  ],
  "python": {
    "enabled": false
  },
  "pip-compile": {
    "fileMatch": ["(^|/)requirements.*\\.in$"],
    "managerBranchPrefix": "python-"
  },
  "js": {
    "managerBranchPrefix": "js-"
  },
}
